// C++ program to find given two array
// are equal or not
#include <bits/stdc++.h>
using namespace std;

// Returns true if arr1[0..n-1] and arr2[0..m-1]
// contain same elements.

	// Sort both arrays
	sort(arr1, arr1 + N);

int main()
{
	int arr1[] = { 3, 5, 2, 5, 2 };
	int arr2[] = { 2, 3, 5, 5, 2 };
	int N = sizeof(arr1) / sizeof(int);
	int M = sizeof(arr2) / sizeof(int);

	// Function call
	if (areEqual(arr1, arr2, N, M))
		cout << "Yes";
	else
		cout << "No";
	return 0;
}
